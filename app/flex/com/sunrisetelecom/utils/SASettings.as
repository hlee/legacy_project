package com.sunrisetelecom.utils
{
    public class SASettings
    {
        private var _rbw:uint;
        private var _vbw:uint;
        private var _ofs:uint;
        private var _att:uint;
        private var _swt:uint;
        private var _span:Number;
        private var _cfreq:Number;
        
        [Bindable("textChange")]
        public function get rbw():uint
        {
            return _rbw;
        }
        
        public function set rbw(val:uint):void
        {
            _rbw=val;
        }
        
        [Bindable]
        public function get vbw():uint
        {
            return _vbw;
        }
        
        public function set vbw(val:uint):void
        {
            _vbw=val;
        }
        
        [Bindable]
        public function get ofs():uint
        {
            return _ofs;
        }
        
        public function set ofs(val:uint):void
        {
            _ofs=val;
        }
        
        [Bindable]
        public function get att():uint
        {
            return _att;
        }
        
        public function set att(val:uint):void
        {
            _att=val;
        }
        
        [Bindable(textChange)]
        public function get swt():uint
        {
            return _swt;
        }
        
        public function set swt(val:uint):void
        {
            _swt=val;
        }
        
        [Bindable]
        public function get span():Number
        {
            return _span;
        }
        
        public function set span(val:Number):void
        {
            _span=val;
        }
        
        [Bindable]
        public function get cfreq():Number
        {
            return _cfreq;
        }
        
        public function set cfreq(val:Number):void
        {
            _cfreq=val;
        }
    }
}
