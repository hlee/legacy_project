<h1>New upstream_channel</h1>

<%= error_messages_for :upstream_channel %>

<% form_for(@upstream_channel) do |f| %>
  <p>
    <b>Name</b><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <b>Freq</b><br />
    <%= f.text_field :freq_disp %>
  </p>

  <p>
    <b>Bandwidth</b><br />
    <%= f.text_field :bandwidth_disp %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', upstream_channels_path %>
